extends editor_layout

block content
	<input type="hidden" id="code" value="#{code}">
	<div class="container-editor">
		<div class="input-code" style="display: none">
			<input type="file" id="openFile" />
			<input id="fileContent" type="hidden" value="">
		</div>
		<div class="split-pane-editor" style="padding-top: 56px">
			<div class="row">
				<div class="col-md-6" style="background: #272822">
					<div class="tab-pane-editor">
						<ul class="nav nav-tabs">
							<li class=""><a data-toggle="tab" href="#home">Principal</a></li>
							<li id="addTabIcon" role="presentation" onclick="Editor.addNewTab()"><a href="#"><i class="fa fa-plus-square" aria-hidden="true"></i>
							</a></li>
							
						</ul>

						<div class="tab-content">
							<div id="home" class="tab-pane fade in active">
								<div id="editor" style="height: 350px">

								</div>
							</div>
						
						</div>
						
					</div>
					
				</div>
				<div class="col-md-6" style="background: #272822">


					<div class="tab-pane-editor">
						<ul class="nav nav-tabs">
							<li class=""><a data-toggle="tab" href="#home">Codigo Intermedio</a></li>
							
						</ul>

						<div class="tab-content">
							<div id="home" class="tab-pane fade in active">
								<div id="intermediate-editor" style="height: 350px">

								</div>
						
						</div>
						
					</div>
					
					
				</div>
			</div>
		</div>
		<div class="output-area">
			<ul class="nav nav-tabs">
				<li class="active"><a data-toggle="tab" href="#console"><i class="fa fa-terminal" aria-hidden="true"></i> Console</a></li>
				<li><a data-toggle="tab" href="#menu1" class="symbol-table-link">Tabla de Simbolos</a></li>
				<li><a data-toggle="tab" href="#menu2" class ="errors-link">Errores</a></li>
				<li><a data-toggle="tab" href="#menu3" class ="stack-link">Stack</a></li>
				<li><a data-toggle="tab" href="#menu4" class ="heap-link">Heap</a></li>
				<li><a data-toggle="tab" href="#menu5" class ="temps-link">Temporales</a></li>
			</ul>

			<div class="tab-content" >
			<div id="console" class="tab-pane fade in active" style="overflow: auto">
				
			</div>
			<div id="menu1" class="tab-pane fade" style="overflow: auto">
				
			</div>
			<div id="menu2" class="tab-pane fade" style="overflow: auto">
				
			</div>
			<div id="menu3" class="tab-pane fade" style="overflow: auto">
				
			</div>
			<div id="menu4" class="tab-pane fade" style="overflow: auto">
				
			</div>
			<div id="menu5" class="tab-pane fade" style="overflow: auto">
				
			</div>
			</div>
		</div>
	</div>
